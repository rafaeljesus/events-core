{
  "name": "@rafaeljesus/events-core",
  "version": "1.0.7",
  "description": "Events core model and services shared by events api and worker",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib/* && babel src --presets es2015,stage-2 --plugins transform-runtime --ignore test -d lib",
    "prepublish": "npm run build",
    "test": " npm run lint && NODE_ENV=test ava",
    "lint": "standard --verbose | snazzy"
  },
  "author": "Rafael Jesus <rafaelljesus86@gmail.com> (http://github.com/rafaeljesus)",
  "license": "MIT",
  "bin": {
    "babel": "./node_modules/babel-cli/bin/babel"
  },
  "pre-commit": [
    "test"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "files": [
      "test/*spec.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {
    "mongoose": "^4.4.7"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "estraverse-fb": "^1.3.1",
    "pre-commit": "^1.1.2",
    "request-promise": "^2.0.1",
    "snazzy": "^3.0.0",
    "standard": "^6.0.7"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
